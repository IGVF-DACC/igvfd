{
    "title": "CuratedSet",
    "description": "Schema for submitting an file set for files originating from external sources.",
    "$id": "/profiles/curated_set.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "required": [
        "title"
    ],
    "identifyingProperties": [
        "uuid",
        "aliases"
    ],
    "additionalProperties": false,
    "mixinProperties": [
        {
            "$ref": "mixins.json#/basic_item"
        },
        {
            "$ref": "mixins.json#/url"
        },
        {
            "$ref": "mixins.json#/accession"
        },
        {
            "$ref": "mixins.json#/standard_status"
        },
        {
            "$ref": "mixins.json#/documents"
        },
        {
            "$ref": "mixins.json#/references"
        },
        {
            "$ref": "mixins.json#/taxa"
        }
    ],
    "properties": {
        "schema_version": {
            "default": "1"
        },
        "accession": {
            "accessionType": "CS"
        },
        "title": {
            "title": "Title",
            "description": "The complete name of the originating lab or vendor.",
            "type": "string",
            "pattern": "^(\\S+(\\s|\\S)*\\S+|\\S)$"
        }
    },
    "changelog": "/profiles/changelogs/source.md"
}
