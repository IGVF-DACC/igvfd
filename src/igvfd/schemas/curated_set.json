{
    "title": "Curated Set",
    "description": "A file set for reference files which are utilized in the analysis of IGVF experiments. For example, genomic references from GENCODE.",
    "$id": "/profiles/curated_set.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "required": [
        "lab",
        "award",
        "file_set_type"
    ],
    "identifyingProperties": [
        "uuid",
        "accession",
        "alternate_accessions",
        "aliases"
    ],
    "additionalProperties": false,
    "mixinProperties": [
        {
            "$ref": "file_set.json#/properties"
        },
        {
            "$ref": "mixins.json#/basic_item"
        },
        {
            "$ref": "mixins.json#/url"
        },
        {
            "$ref": "mixins.json#/accession"
        },
        {
            "$ref": "mixins.json#/attribution"
        },
        {
            "$ref": "mixins.json#/documents"
        },
        {
            "$ref": "mixins.json#/publications"
        },
        {
            "$ref": "mixins.json#/taxa"
        },
        {
            "$ref": "mixins.json#/release_timestamp"
        }
    ],
    "dependentSchemas": {
        "$merge": [
            "shared_dependencies.json#/accession_status",
            "file_set.json#/dependentSchemas"
        ]
    },
    "properties": {
        "schema_version": {
            "default": "8"
        },
        "file_set_type": {
            "description": "The category that best describes this curated file set.",
            "comment": "File set type describes the content of the curated set.",
            "enum": [
                "barcodes",
                "editing templates",
                "elements",
                "external data for catalog",
                "genome",
                "genes",
                "guide RNAs",
                "transcriptome",
                "variants"
            ],
            "enum_descriptions": {
                "barcodes": "Barcodes are lists of barcodes.",
                "editing templates": "Editing templates are used in Saturation Genome Editing (SGE) assays.",
                "elements": "Elements are lists of elements in the genome.",
                "external data for catalog": "External data for catalog are reference files from other sources utilized in the IGVF catalog.",
                "genome": "Genome file sets are genome references.",
                "genes": "Genes are lists of genes.",
                "guide RNAs": "Guide RNAs are guides used in CRISPR screen assays.",
                "transcriptome": "Transcriptome file sets are transcriptome references.",
                "variants": "Variants are lists of variants."
            },
            "submissionExample": {
                "appscript": "guide RNAs",
                "igvf_utils": "guide RNAs"
            }
        }
    },
    "fuzzy_searchable_fields": [
        "@type",
        "collections",
        "description",
        "file_set_type",
        "files.content_type",
        "files.submitted_file_name",
        "lab.title",
        "summary",
        "samples.summary",
        "samples.sample_terms.term_name",
        "samples.targeted_sample_term.term_name",
        "submitted_by.title",
        "taxa"
    ],
    "exact_searchable_fields": [
        "control_for.accession",
        "control_for.aliases",
        "dbxrefs",
        "files.accession",
        "files.aliases",
        "publications.publication_identifiers",
        "donors.accession",
        "donors.aliases",
        "samples.accession",
        "samples.aliases",
        "assemblies",
        "transcriptome_annotations"
    ],
    "changelog": "/profiles/changelogs/curated_set.md"
}
