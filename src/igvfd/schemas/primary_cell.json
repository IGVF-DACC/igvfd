{
    "title": "Primary Cell",
    "$id": "/profiles/primary_cell.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "description": "Schema for submitting a primary cell sample.",
    "type": "object",
    "required": [
        "award",
        "lab",
        "source"
    ],
    "identifyingProperties": [
        "uuid",
        "accession",
        "alternate_accessions",
        "aliases"
    ],
    "additionalProperties": false,
    "mixinProperties": [
        {
            "$ref": "sample.json#/properties"
        },
        {
            "$ref": "biosample.json#/properties"
        },
        {
            "$ref": "mixins.json#/basic_item"
        },
        {
            "$ref": "mixins.json#/accession"
        },
        {
            "$ref": "mixins.json#/attribution"
        },
        {
            "$ref": "mixins.json#/documents"
        },
        {
            "$ref": "mixins.json#/product_info"
        },
        {
            "$ref": "mixins.json#/url"
        },
        {
            "$ref": "mixins.json#/taxa"
        }
    ],
    "dependentSchemas": {
        "$merge": [
            "biosample.json#/dependentSchemas"
        ]
    },
    "properties": {
        "schema_version": {
            "default": "1"
        },
        "passage_number": {
            "title": "Passage number",
            "description": "Number of passages including the passages from the source.",
            "comment": "In calculating passage number, include passages from the source.",
            "type": "integer",
            "minimum": 0
        },
        "date_obtained": {
            "title": "Date harvested"
        },
        "pooled_from": {
            "items": {
                "linkTo": "PrimaryCell",
                "type": "string"
            }
        },
        "cell_surface_molecules": {
            "title": "Cell surface molecules",
            "description": "List of cell surface molecules that describe the immunophenotype of the sample.",
            "type": "array",
            "uniqueItems": true,
            "items": {
                 "title": "Cell surface molecule",
                 "type": "string",
                 "enum": [
                     "CCR4-",
                     "CCR4+",
                     "CCR6-",
                     "CCR6+",
                     "CD103-",
                     "CD103+",
                     "CD115-",
                     "CD115+",
                     "CD117-",
                     "CD117+",
                     "CD11b-",
                     "CD11b+",
                     "CD11c-",
                     "CD11c+",
                     "CD14-",
                     "CD14+",
                     "CD141-",
                     "CD141+",
                     "CD16-",
                     "CD16+",
                     "CD161-",
                     "CD161+",
                     "CD19-",
                     "CD19+",
                     "CD1a-",
                     "CD1a+",
                     "CD1c-",
                     "CD1c+",
                     "CD2-",
                     "CD2+",
                     "CD217-",
                     "CD217+",
                     "CD24-",
                     "CD24+",
                     "CD25-",
                     "CD25+",
                     "CD27-",
                     "CD27+",
                     "CD28-",
                     "CD28+",
                     "CD34-",
                     "CD34+",
                     "CD38-",
                     "CD38+",
                     "CD4-",
                     "CD4+",
                     "CD41-",
                     "CD41+",
                     "CD42-",
                     "CD42+",
                     "CD43-",
                     "CD43+",
                     "CD44-",
                     "CD44+",
                     "CD45RA-",
                     "CD45RA+",
                     "CD45RO-",
                     "CD45RO+",
                     "CD5-",
                     "CD5+",
                     "CD56-",
                     "CD56+",
                     "CD69-",
                     "CD69+",
                     "CD7-",
                     "CD7+",
                     "CD8-",
                     "CD8+",
                     "CD86-",
                     "CD86+",
                     "CD9-",
                     "CD9+",
                     "CD94-",
                     "CD94+",
                     "CXCR3-",
                     "CXCR3+"
                 ]
             }
        }
    },
    "changelog": "/profiles/changelogs/primary_cell.md"
}
