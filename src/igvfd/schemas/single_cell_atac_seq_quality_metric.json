{
    "title": "Single Cell ATAC-seq Quality Metric",
    "$id": "/profiles/single_cell_atac_seq_quality_metric.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "description": "Schema for submission of a scATAC-seq uniform pipeline quality metric.",
    "type": "object",
    "required": [
        "award",
        "lab",
        "quality_metric_of",
        "analysis_step_version"
    ],
    "identifyingProperties": [
        "uuid",
        "aliases"
    ],
    "additionalProperties": false,
    "mixinProperties": [
        {
            "$ref": "quality_metric.json#/properties"
        },
        {
            "$ref": "mixins.json#/basic_item"
        },
        {
            "$ref": "mixins.json#/attribution"
        },
        {
            "$ref": "mixins.json#/attachment"
        },
        {
            "$ref": "mixins.json#/release_timestamp"
        },
        {
            "$ref": "mixins.json#/standard_status"
        }
    ],
    "dependentSchemas": {
        "$merge": [
            "quality_metric.json#/dependentSchemas"
        ]
    },
    "properties": {
        "schema_version": {
            "default": "1"
        },
        "tsse": {
            "title": "Transcription Start Site Enrichment",
            "description": "",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "n_fragments": {
            "title": "Number Of Fragments",
            "description": "",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "n_barcodes": {
            "title": "Number Of Barcodes",
            "description": "",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "pct_duplicates": {
            "title": "Percent Duplicates",
            "description": "",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "n_fragment": {
            "title": "Number Of Fragments",
            "description": "",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "frac_dup": {
            "title": "Duplicate Fraction",
            "description": "",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "frac_mito": {
            "title": "Mitochondrial Fraction",
            "description": "",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "duplicate": {
            "title": "Duplicate Number",
            "description": "",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "unmapped": {
            "title": "Unmapped Number",
            "description": "",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "lowmapq": {
            "title": "Low Mapping Quality",
            "description": "",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "joint_barcodes_passing": {
            "title": "Joint Barcodes Passing",
            "description": "",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "n_reads": {
            "title": "Number Of Reads",
            "description": "Total count of sequencing reads processed.",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "n_mapped_reads": {
            "title": "Number Of Mapped Reads",
            "description": "Reads successfully aligned to the reference genome.",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "n_uniquely_mapped_reads": {
            "title": "Number Of Uniquely Mapped Reads",
            "description": "Reads aligned to a single location in the genome.",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "n_reads_with_multi_mappings": {
            "title": "Number Of Reads With Multi-Mappings",
            "description": "Reads aligned to multiple locations in the genome.",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "n_candidates": {
            "title": "Number Of Candidates",
            "description": "Potential mapping locations considered during alignment.",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "n_mappings": {
            "title": "Number Of Mappings",
            "description": "Total successful alignments, including multiple mappings per read.",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "n_uni_mappings": {
            "title": "Number Of Uni-Mappings",
            "description": "Total alignments where reads map to a single location.",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "n_multi_mappings": {
            "title": "Number Of Multi-Mappings",
            "description": "Total alignments where reads map to multiple locations.",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "n_barcodes_on_onlist": {
            "title": "Number Of Barcodes On Onlist",
            "description": "Barcodes matching the expected list of valid barcodes.",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "n_corrected_barcodes": {
            "title": "Number Of Corrected Barcodes",
            "description": "Barcodes adjusted to match valid entries in the onlist.",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "n_output_mappings": {
            "title": "Number Of Output Mappings",
            "description": "Final count of fragments after deduplication and filtering.",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "uni_mappings": {
            "title": "Uni-Mappings",
            "description": "Number of fragments mapping to single location in the genome.",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "multi_mappings": {
            "title": "Multi-Mappings",
            "description": "Number of fragments mappig in multiple locations in the genome.",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        },
        "total": {
            "title": "Total",
            "description": "Sum of uni-mappings and multi-mappings.",
            "type": "number",
            "submissionExample": {
                "appscript": 8,
                "igvf_utils": 8
            }
        }
    },
    "fuzzy_searchable_fields": [
        "aliases",
        "description",
        "lab.title",
        "submitted_by.title",
        "summary"
    ],
    "exact_searchable_fields": [
        "@type",
        "quality_metric_of"
    ],
    "changelog": "/profiles/changelogs/single_cell_atac_seq_quality_metric.md"
}
