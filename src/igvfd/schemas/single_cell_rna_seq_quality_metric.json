    "description": "Schema for submission of a scRNA-seq uniform pipeline quality metric.",
    "type": "object",
    "required": [
        "award",
        "lab",
        "quality_metric_of",
        "analysis_step_version"
    ],
    "identifyingProperties": [
        "uuid",
        "aliases"
    ],
    "additionalProperties": false,
