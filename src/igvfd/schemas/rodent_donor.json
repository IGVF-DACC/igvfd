{
    "title": "Rodent Donor",
    "$id": "/profiles/rodent_donor.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "description": "Derived schema for submitting rodent donors.",
    "type": "object",
    "required": [
        "award",
        "lab",
        "taxa",
        "sex",
        "strain"
    ],
    "identifyingProperties": [
        "uuid",
        "accession",
        "alternate_accession",
        "alias"
    ],
    "additionalProperties": false,
    "mixinProperties": [
        {
            "$ref": "donor.json#/properties"
        },
        {
            "$ref": "mixins.json#/basic_item"
        },
        {
            "$ref": "mixins.json#/accession"
        },
        {
            "$ref": "mixins.json#/attribution"
        },
        {
            "$ref": "mixins.json#/document"
        },
        {
            "$ref": "mixins.json#/product_info"
        },
        {
            "$ref": "mixins.json#/url"
        },
        {
            "$ref": "mixins.json#/reference"
        },
        {
            "$ref": "mixins.json#/taxa"
        }
    ],
    "dependentSchemas": {
        "lot_id": {
            "required": [
                "product_id"
            ],
            "comment": "Donor with lot_id must include product_id."
        }
    },
    "properties": {
        "schema_version": {
            "default": "2"
        },
        "parents": {
            "items": {
                "linkTo": "RodentDonor",
                "type": "string"
            }
        },
        "strain_background": {
            "title": "Strain Background",
            "description": "The specific parent strain designation of a non-human donor.",
            "comment": "Strain background of rodent strains.  See Ali Mortazavi grant.",
            "type": "string",
            "enum": [
                "A/J (AJ)",
                "C57BL/6J (B6)",
                "129S1/SvImJ (129)",
                "NOD/ShiLtJ (NOD)",
                "NZO/H1LtJ (NZO)",
                "CAST/EiJ (CAST)",
                "PWK/PhJ (PWK)",
                "WSB/EiJ (WSB)",
                "CAST (M. m. castaneus)",
                "WSB (M. m. domesticus)",
                "PWK (M. m. musculus)"
            ]
        },
        "strain": {
            "title": "Strain Name",
            "description": "The specific strain designation of a non-human donor.",
            "comment": "Strain name of rodent strains.",
            "type": "string"
        },
        "genotype": {
            "title": "Strain Genotype",
            "description": "The genotype of the strain according to accepted nomenclature conventions.",
            "comment": "Genotype of rodent strains.",
            "type": "string"
        },
        "taxa": {
            "enum": [
                "Mus musculus"
            ]
        },
        "trait": {
            "description": "A list of known phenotypic traits of the Rodent donor",
            "type": "array"
        }
    },
    "columns": {
        "uuid": {
            "title": "UUID"
        },
        "accession": {
            "title": "Accession"
        },
        "alias": {
            "title": "Alias"
        },
        "taxa": {
            "title": "Taxa"
        },
        "sex": {
            "title": "Sex"
        },
        "award": {
            "title": "Award"
        },
        "strain": {
            "title": "Strain"
        },
        "lab": {
            "title": "Lab"
        },
        "status": {
            "title": "Status"
        },
        "submitted_by": {
            "title": "Submitted By"
        }
    },
    "boost_values": {
        "accession": 1.0,
        "@type": 1.0,
        "taxa": 1.0,
        "strain": 1.0
    },
    "changelog": "/profiles/changelogs/rodent_donor.md"
}
