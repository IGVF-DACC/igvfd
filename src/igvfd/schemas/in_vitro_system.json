{
    "title": "In Vitro System",
    "$id": "/profiles/in_vitro_system.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "description": "A biosample that is cultured, such as immortalized cell lines, organoids, or samples that have been differentiated or reprogrammed.",
    "type": "object",
    "required": [
        "award",
        "lab",
        "sources",
        "donors",
        "sample_terms",
        "classifications"
    ],
    "identifyingProperties": [
        "uuid",
        "accession",
        "alternate_accessions",
        "aliases"
    ],
    "additionalProperties": false,
    "mixinProperties": [
        {
            "$ref": "sample.json#/properties"
        },
        {
            "$ref": "biosample.json#/properties"
        },
        {
            "$ref": "mixins.json#/basic_item"
        },
        {
            "$ref": "mixins.json#/accession"
        },
        {
            "$ref": "mixins.json#/attribution"
        },
        {
            "$ref": "mixins.json#/documents"
        },
        {
            "$ref": "mixins.json#/product_info"
        },
        {
            "$ref": "mixins.json#/url"
        },
        {
            "$ref": "mixins.json#/taxa"
        },
        {
            "$ref": "mixins.json#/publication_identifiers"
        },
        {
            "$ref": "mixins.json#/release_timestamp"
        }
    ],
    "dependentSchemas": {
        "$merge": [
            "biosample.json#/dependentSchemas"
        ],
        "passage_number": {
            "comment": "Only in vitro system with classification as a cell line are allowed to have passage_number property.",
            "properties": {
                "classifications": {
                    "contains": {
                        "enum": [
                            "cell line"
                        ]
                    }
                }
            }
        },
        "classifications": {
            "comment": "If an in vitro system has classification organoid, differentiated cell specimen, or reprogrammed cell specimen then targeted_sample_term, cell_fate_change_treatments, time_post_change, and time_post_change_units are required.",
            "allOf": [
                {
                    "if": {
                        "properties": {
                            "classifications": {
                                "contains": {
                                    "enum": [
                                        "differentiated cell specimen",
                                        "organoid",
                                        "reprogrammed cell specimen"
                                    ]
                                }
                            }
                        }
                    },
                    "then": {
                        "oneOf": [
                            {
                                "required": [
                                    "time_post_change",
                                    "time_post_change_units",
                                    "cell_fate_change_treatments",
                                    "targeted_sample_term"
                                ]
                            },
                            {
                                "required": [
                                    "time_post_change",
                                    "time_post_change_units",
                                    "cell_fate_change_protocol",
                                    "targeted_sample_term"
                                ]
                            }
                        ]
                    }
                },
                {
                    "if": {
                        "properties": {
                            "classifications": {
                                "contains": {
                                    "enum": [
                                        "cell line"
                                    ]
                                }
                            }
                        }
                    },
                    "then": {
                        "not": {
                            "anyOf": [
                                {
                                    "required": [
                                        "cell_fate_change_protocol"
                                    ]
                                },
                                {
                                    "required": [
                                        "cell_fate_change_treatments"
                                    ]
                                },
                                {
                                    "required": [
                                        "targeted_sample_term"
                                    ]
                                },
                                {
                                    "required": [
                                        "time_post_change"
                                    ]
                                },
                                {
                                    "required": [
                                        "time_post_change_units"
                                    ]
                                }
                            ]
                        }
                    }
                },
                {
                    "oneOf": [
                        {
                            "properties": {
                                "classifications": {
                                    "contains": {
                                        "enum": [
                                            "cell line"
                                        ]
                                    },
                                    "not": {
                                        "contains": {
                                            "enum": [
                                                "organoid",
                                                "gastruloid",
                                                "embryoid",
                                                "differentiated cell specimen",
                                                "reprogrammed cell specimen",
                                                "pooled cell specimen"
                                            ]
                                        }
                                    }
                                }
                            }
                        },
                        {
                            "properties": {
                                "classifications": {
                                    "contains": {
                                        "enum": [
                                            "organoid"
                                        ]
                                    },
                                    "not": {
                                        "contains": {
                                            "enum": [
                                                "cell line",
                                                "gastruloid",
                                                "embryoid",
                                                "differentiated cell specimen",
                                                "reprogrammed cell specimen",
                                                "pooled cell specimen"
                                            ]
                                        }
                                    }
                                }
                            }
                        },
                        {
                            "properties": {
                                "classifications": {
                                    "contains": {
                                        "enum": [
                                            "gastruloid"
                                        ]
                                    },
                                    "not": {
                                        "contains": {
                                            "enum": [
                                                "cell line",
                                                "organoid",
                                                "embryoid",
                                                "differentiated cell specimen",
                                                "reprogrammed cell specimen",
                                                "pooled cell specimen"
                                            ]
                                        }
                                    }
                                }
                            }
                        },
                        {
                            "properties": {
                                "classifications": {
                                    "contains": {
                                        "enum": [
                                            "embryoid"
                                        ]
                                    },
                                    "not": {
                                        "contains": {
                                            "enum": [
                                                "cell line",
                                                "organoid",
                                                "gastruloid",
                                                "differentiated cell specimen",
                                                "reprogrammed cell specimen",
                                                "pooled cell specimen"
                                            ]
                                        }
                                    }
                                }
                            }
                        },
                        {
                            "properties": {
                                "classifications": {
                                    "contains": {
                                        "enum": [
                                            "differentiated cell specimen"
                                        ]
                                    },
                                    "not": {
                                        "contains": {
                                            "enum": [
                                                "cell line",
                                                "organoid",
                                                "gastruloid",
                                                "embryoid",
                                                "reprogrammed cell specimen",
                                                "pooled cell specimen"
                                            ]
                                        }
                                    }
                                }
                            }
                        },
                        {
                            "properties": {
                                "classifications": {
                                    "contains": {
                                        "enum": [
                                            "reprogrammed cell specimen"
                                        ]
                                    },
                                    "not": {
                                        "contains": {
                                            "enum": [
                                                "cell line",
                                                "organoid",
                                                "gastruloid",
                                                "embryoid",
                                                "differentiated cell specimen",
                                                "pooled cell specimen"
                                            ]
                                        }
                                    }
                                }
                            }
                        },
                        {
                            "properties": {
                                "classifications": {
                                    "contains": {
                                        "enum": [
                                            "pooled cell specimen"
                                        ]
                                    },
                                    "not": {
                                        "contains": {
                                            "enum": [
                                                "cell line",
                                                "organoid",
                                                "gastruloid",
                                                "embryoid",
                                                "differentiated cell specimen",
                                                "reprogrammed cell specimen"
                                            ]
                                        }
                                    }
                                }
                            }
                        },
                        {
                            "properties": {
                                "classifications": {
                                    "allOf": [
                                        {
                                            "contains": {
                                                "enum": [
                                                    "differentiated cell specimen"
                                                ]
                                            }
                                        },
                                        {
                                            "contains": {
                                                "enum": [
                                                    "pooled cell specimen"
                                                ]
                                            }
                                        }
                                    ],
                                    "not": {
                                        "contains": {
                                            "enum": [
                                                "cell line",
                                                "organoid",
                                                "gastruloid",
                                                "embryoid",
                                                "reprogrammed cell specimen"
                                            ]
                                        }
                                    }
                                }
                            }
                        },
                        {
                            "properties": {
                                "classifications": {
                                    "allOf": [
                                        {
                                            "contains": {
                                                "enum": [
                                                    "reprogrammed cell specimen"
                                                ]
                                            }
                                        },
                                        {
                                            "contains": {
                                                "enum": [
                                                    "pooled cell specimen"
                                                ]
                                            }
                                        }
                                    ],
                                    "not": {
                                        "contains": {
                                            "enum": [
                                                "cell line",
                                                "organoid",
                                                "gastruloid",
                                                "embryoid",
                                                "differentiated cell specimen"
                                            ]
                                        }
                                    }
                                }
                            }
                        }
                    ]
                }
            ]
        },
        "time_post_change": {
            "comment": "Specification of time_post_change requires time_post_change_units and cell_fate_change_treatments.",
            "oneOf": [
                {
                    "required": [
                        "time_post_change_units",
                        "cell_fate_change_treatments"
                    ]
                },
                {
                    "required": [
                        "time_post_change_units",
                        "cell_fate_change_protocol"
                    ]
                }
            ]
        },
        "time_post_change_units": {
            "comment": "Specification of time_post_change_units requires time_post_change and cell_fate_change_treatments.",
            "oneOf": [
                {
                    "required": [
                        "time_post_change",
                        "cell_fate_change_treatments"
                    ]
                },
                {
                    "required": [
                        "time_post_change",
                        "cell_fate_change_protocol"
                    ]
                }
            ]
        },
        "cell_fate_change_treatments": {
            "comment": "Specification of cell_fate_change_treatments requires time_post_change_units and time_post_change.",
            "required": [
                "time_post_change_units",
                "time_post_change"
            ]
        },
        "cell_fate_change_protocol": {
            "comment": "Specification of cell_fate_change_protocol requires time_post_change_units and time_post_change.",
            "required": [
                "time_post_change_units",
                "time_post_change"
            ]
        }
    },
    "properties": {
        "schema_version": {
            "default": "19"
        },
        "classifications": {
            "title": "Classifications",
            "description": "The general category of this type of in vitro system.",
            "comment": "The specified category should match the ontology type used to describe this type of in vitro system. For example 'brain organoid' uses UBERON ontology, cell lines use CL ontology, etc. These terms are in concordance with NCIT definitions.",
            "type": "array",
            "minItems": 1,
            "maxItems": 2,
            "uniqueItems": true,
            "items": {
                "title": "Classification",
                "description": "The type of the in vitro system.",
                "type": "string",
                "enum": [
                    "organoid",
                    "gastruloid",
                    "embryoid",
                    "cell line",
                    "differentiated cell specimen",
                    "reprogrammed cell specimen",
                    "pooled cell specimen"
                ]
            },
            "submissionExample": {
                "appscript": "[\"differentiated cell specimen\"]",
                "igvf_utils": "differentiated cell specimen"
            }
        },
        "time_post_change": {
            "title": "Time Post Change",
            "description": "The time that elapsed past the time-point when the cell fate change treatments were introduced.",
            "type": "number",
            "submissionExample": {
                "appscript": 27,
                "igvf_utils": 27
            }
        },
        "time_post_change_units": {
            "title": "Time Post Change Units",
            "description": "The units of time that elapsed past the point when the cell fate change treatments were introduced.",
            "type": "string",
            "enum": [
                "minute",
                "hour",
                "day",
                "week",
                "month"
            ],
            "submissionExample": {
                "appscript": "minute",
                "igvf_utils": "minute"
            }
        },
        "date_obtained": {
            "title": "Date Collected",
            "submissionExample": {
                "appscript": "2022-02-20",
                "igvf_utils": "2022-02-20"
            }
        },
        "cell_fate_change_treatments": {
            "title": "Cell Fate Change Treatments",
            "description": "A list of treatments applied to the biosample with the purpose of differentiation, dedifferentiation, or reprogramming.",
            "comment": "This property is now deprecated. Cell_fate_change_protocol should be used instead.",
            "type": "array",
            "minItems": 1,
            "uniqueItems": true,
            "items": {
                "title": "Treatment",
                "description": "A treatment applied to the biosample.",
                "type": "string",
                "linkTo": "Treatment"
            },
            "permission": "import_items",
            "submissionExample": {
                "appscript": "[\"john-doe:treatment01\", \"/treatments/ffe525b2-bd67-4021-835e-29748513b560/\"]",
                "igvf_utils": "john-doe:treatment01, /treatments/ffe525b2-bd67-4021-835e-29748513b560/"
            }
        },
        "cell_fate_change_protocol": {
            "title": "Cell Fate Change Protocol",
            "description": "A protocol applied to the biosample with the purpose of differentiation, dedifferentiation, or reprogramming.",
            "type": "string",
            "linkTo": "Document",
            "submissionExample": {
                "appscript": "john-doe:document01 or /documents/1dd3f63d-13b5-08f8-14b2-2974bd674f59/",
                "igvf_utils": "john-doe:document01 or /documents/1dd3f63d-13b5-08f8-14b2-2974bd674f59/"
            }
        },
        "originated_from": {
            "title": "Originated From",
            "description": "The biosample this in vitro system sample was originated from due to differentiation, dedifferentiation, or reprogramming.",
            "type": "string",
            "linkTo": "Biosample",
            "submissionExample": {
                "appscript": "TSTSM02881437 or john-doe:tissue01",
                "igvf_utils": "TSTSM02881437 or john-doe:tissue01"
            }
        },
        "part_of": {
            "linkTo": "Biosample",
            "type": "string",
            "submissionExample": {
                "appscript": "TSTSM02881436 or john-doe:tissue02",
                "igvf_utils": "TSTSM02881436 or john-doe:tissue02"
            }
        },
        "pooled_from": {
            "items": {
                "linkTo": "Biosample",
                "type": "string"
            },
            "submissionExample": {
                "appscript": "TSTSM02881435 or john-doe:tissue03",
                "igvf_utils": "TSTSM02881435 or john-doe:tissue03"
            }
        },
        "passage_number": {
            "title": "Passage Number",
            "description": "Number of passages including the passages from the source.",
            "comment": "In calculating passage number, include passages from the source.",
            "type": "integer",
            "minimum": 0,
            "submissionExample": {
                "appscript": 3,
                "igvf_utils": 3
            }
        },
        "targeted_sample_term": {
            "title": "Targeted Sample Term",
            "description": "Ontology term identifying the targeted endpoint biosample resulting from differentation or reprogramming.",
            "type": "string",
            "linkTo": "SampleTerm",
            "submissionExample": {
                "appscript": "/sample-terms/UBERON_0002367/",
                "igvf_utils": "/sample-terms/UBERON_0002367/"
            }
        }
    },
    "fuzzy_searchable_fields": [
        "classifications",
        "taxa"
    ],
    "exact_searchable_fields": [
        "@type",
        "dbxrefs",
        "nih_institutional_certification"
    ],
    "changelog": "/profiles/changelogs/in_vitro_system.md"
}
