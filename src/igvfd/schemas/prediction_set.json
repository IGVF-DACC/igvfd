{
    "title": "Prediction Set",
    "description": "Schema for submitting a file set for files or predictions generated by CAMP.",
    "$id": "/profiles/prediction_set.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "required": [
        "lab",
        "award",
        "prediction_type"
    ],
    "identifyingProperties": [
        "uuid",
        "aliases"
    ],
    "additionalProperties": false,
    "mixinProperties": [
        {
            "$ref": "file_set.json#/properties"
        },
        {
            "$ref": "mixins.json#/basic_item"
        },
        {
            "$ref": "mixins.json#/url"
        },
        {
            "$ref": "mixins.json#/accession"
        },
        {
            "$ref": "mixins.json#/attribution"
        },
        {
            "$ref": "mixins.json#/documents"
        },
        {
            "$ref": "mixins.json#/references"
        }
    ],
    "dependentSchemas": {
        "$merge": [
            "file_set.json#/dependentSchemas"
        ]
    },
    "properties": {
        "schema_version": {
            "default": "1"
        },
        "prediction_type": {
            "title": "Prediction Type",
            "description": "The category that best describes this prediction file set.",
            "type": "string",
            "enum": [
                "pathogenicity",
                "functional effect",
                "protein stability",
                "activity level"
            ]
        }
    },
    "fuzzy_searchable_fields": [
        "prediction_type",
        "lab.title"
    ],
    "exact_searchable_fields": [
        "@type",
        "dbxrefs"
    ],
    "changelog": "/profiles/changelogs/prediction_set.md"
}
